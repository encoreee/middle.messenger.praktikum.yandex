<div class="{{styles.message-area__conversation-area__my-message-wrapper}}">
  <div class="{{styles.message-area__conversation-area__my-message}}">
    {{text}}
    <div class="{{styles.message-area__conversation-area__info}}">
      <img
        class="{{styles.message-area__conversation-area__checked}}"
        src="../../static/checked.svg"
        alt="checked"
        width="15"
        height="15"
      />
      <div class="{{styles.message-area__conversation-area__my-time}}">
        {{time}}
      </div>
    </div>
  </div>
</div>


