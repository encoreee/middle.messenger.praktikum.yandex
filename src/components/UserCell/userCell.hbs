<div class="{{styles.users-area__user-cell}}">
  <div class="{{styles.users-area__user-cell__avatar}}">
  </div>
  <div class="{{styles.users-area__user-cell__data}}">
    <div class="{{styles.users-area__user-cell__data__name-label}}"><b
      >{{name}}</b></div>
    <div
      class="{{styles.users-area__user-cell__data__messege-label}}"
    >{{message}}</div>
  </div>
  <div class="{{styles.users-area__user-cell__info}}">
    <div
      class="{{styles.users-area__user-cell__info__time-label}}"
    >{{time}}</div>
    <div class="{{styles.users-area__user-cell__info__count-label}}">
      <div class="{{styles.users-area__user-cell__info__count-label__text}}">
        {{messageCount}}
      </div>
    </div>
  </div>
</div>